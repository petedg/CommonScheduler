<UserControl x:Class="CommonScheduler.ContentComponents.GlobalAdmin.Controls.SuperAdminDataGridControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             xmlns:mycomp="clr-namespace:CommonScheduler.ContentComponents">
    <Grid Name="grid">
        <mycomp:DataGridControl x:Name="dataGrid" RowEditEnding="dataGrid_RowEditEnding" PreviewKeyDown="dataGrid_PreviewKeyDown" 
                                SelectionMode="Single" SelectedCellsChanged="dataGrid_SelectedCellsChanged">
            <!--DataGridTextColumn Header="NAME" Width="Auto" IsReadOnly="False" Binding="{Binding NAME}"/>
        <DataGridTextColumn Header="SURNAME" Width="Auto" IsReadOnly="False" Binding="{Binding SURNAME}"/>
        <DataGridTextColumn Header="LOGIN" Width="Auto" IsReadOnly="True" Binding="{Binding LOGIN}" /-->
        </mycomp:DataGridControl>
    </Grid>
</UserControl>
